{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "analyze": {
      "cache": false
    },
    "setup": {
      "cache": false,
      "outputs": [".env.local", ".env.test.local"]
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "sanity": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:apps": {
      "cache": false
    },
    "dev:packages": {
      "cache": false
    },
    "dev:test": {
      "dependsOn": ["build:packages"]
    },
    "prebuild": {
      "dependsOn": ["^prebuild"],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "postbuild": {
      "dependsOn": ["^postbuild"],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "typecheck": {
      "outputs": ["tsconfig.tsbuildinfo"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^test"],
      "outputs": []
    },
    "test:headless": {
      "cache": false,
      "dependsOn": ["^test:headless"],
      "outputs": ["coverage/**", ".nyc_output/**"]
    },
    "test:ci": {
      "cache": false,
      "dependsOn": ["^test:ci"],
      "outputs": ["coverage/**", ".nyc_output/**"]
    }
  }
}
