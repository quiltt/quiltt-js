{
  "name": "@quiltt/core",
  "version": "3.6.1",
  "description": "Javascript API client and utilities for Quiltt",
  "keywords": [
    "quiltt",
    "typescript"
  ],
  "homepage": "https://github.com/quiltt/quiltt-js/tree/main/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/quiltt/quiltt-js.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "sideEffects": [
    "./src/Storage/Local.ts",
    "./src/api/graphql/links/SubscriptionLink.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "bunchee",
    "clean": "rimraf .turbo dist",
    "dev": "bunchee --watch",
    "lint": "TIMING=1 eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "cross-fetch": "^4.0.0",
    "graphql": "^16.8.1",
    "graphql-ruby-client": "^1.13.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.12.2",
    "@types/react": "18.2.73",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "bunchee": "4.4.8",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.8",
    "rimraf": "5.0.5",
    "typescript": "5.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
