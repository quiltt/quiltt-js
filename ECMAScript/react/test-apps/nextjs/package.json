{
  "name": "@quiltt/react-test-nextjs",
  "description": "Quiltt's React SDK integrated into Next.js",
  "version": "3.1.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress": "pnpm run cypress:install && pnpm run cypress:open",
    "cypress:component": "pnpm run cypress:install && cypress run --component --browser chrome",
    "test": "pnpm run cypress:component"
  },
  "dependencies": {
    "@quiltt/react": "workspace:*",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.11.4",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "cypress": "^12.16.0",
    "encoding": "^0.1.13",
    "eslint": "8.44.0",
    "eslint-config-next": "14.0.4",
    "postcss": "8.4.24",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  }
}
