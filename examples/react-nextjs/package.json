{
  "name": "@quiltt/examples-nextjs",
  "version": "1.0.0",
  "private": true,
  "description": "Quiltt's React SDK integrated into Next.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "cypress": "pnpm run cypress:install && pnpm run cypress:open",
    "cypress:component": "pnpm run cypress:install && cypress run --component --browser chrome",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "dev": "next dev",
    "lint": "TIMING=1 biome check app/ --fix",
    "start": "next start",
    "test": "pnpm run cypress:component",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@quiltt/react": "workspace:*",
    "next": "15.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.15.31",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "autoprefixer": "10.4.21",
    "cypress": "13.17.0",
    "encoding": "0.1.13",
    "postcss": "8.5.5",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
