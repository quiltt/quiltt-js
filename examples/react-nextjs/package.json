{
  "name": "@quiltt/examples-nextjs",
  "version": "1.0.0",
  "private": true,
  "description": "Quiltt's React SDK integrated into Next.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "cypress": "pnpm run cypress:install && pnpm run cypress:open",
    "cypress:component": "pnpm run cypress:install && cypress run --component --browser chrome",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "dev": "next dev",
    "lint": "TIMING=1 biome check app/ --fix",
    "start": "next start",
    "test": "pnpm run cypress:component",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@quiltt/react": "workspace:*",
    "next": "14.2.5",
    "node-polyfill-webpack-plugin": "4.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "@types/node": "22.4.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "cypress": "12.17.4",
    "encoding": "0.1.13",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
